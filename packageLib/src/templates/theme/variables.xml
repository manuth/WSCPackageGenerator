<?xml version="1.0" encoding="UTF-8" ?>
<variables xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" <%
%>xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/tornado/styleVariables.xsd"><%
%><% for (let variableName in Instruction.Theme.Variables) { %>
    <variable name="<%= variableName %>"><%
        %><%= Instruction.Theme.Variables[variableName] %><%
    %></variable><%
%><% } %><%
%><% if (Instruction.Theme.CustomScss) { %>
    <variable name="individualScss"><%
        %><%= Instruction.Theme.CustomScss %><%
    %></variable><%
%><% } %><%
%><% if (Instruction.Theme.OverrideScss) { %>
    <variable name="overrideScss"><%
        %><%= Instruction.Theme.OverrideScss %><%
    %></variable><%
%><% } %>
</variables>