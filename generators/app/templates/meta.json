{
    "Identifier": <%- JSON.stringify(identifier) %>,
    "Name": {
        "en": <%- JSON.stringify(name) %>
    },
    "Date": <%- JSON.stringify(new Date()) %>,
    "Description": {
        "en": <%- JSON.stringify(description) %>
    }<% for (component of components) {
        switch (component)
        {
            case 'files':%>,
    "Files": <%- JSON.stringify(sourcePath) %><%
                break;
            case 'acp':%>,
    "EventListeners": <%- JSON.stringify(optionsFile) %><%
                break;
            case 'eventListener':%>,
    "Options": <%- JSON.stringify(eventListenerFile) %><%
                break;
            case 'translations':%>,
    "Translations": <%- JSON.stringify(translationsFile) %><%
                break;
            case 'errors':%>,
    "Errors": <%- JSON.stringify(errorMessageFile) %><%
                break;
            case 'style':%>,
    "Styles": <%- JSON.stringify(stylePath) %><%
                break;
            case 'template':%>,
    "Templates": <%- JSON.stringify(templatePath) %><%
                break;
            case 'acpTemplate':%>,
    "ACPTemplates": <%- JSON.stringify(acpTemplatePath) %><%
                break;
            case 'templateListener':%>,
    "TemplateListener": <%- JSON.stringify(templateListenerFile) %><%
                break;
            case 'emoji':%>,
    "Emojis": <%- JSON.stringify(emojiFile) %><%
                break;
        }
    }%>
}