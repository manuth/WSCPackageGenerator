    Name: "<%= name  %>",
    DisplayName: {
        en: "<%= displayName %>"
    },
    Description: {
        en: "<%= description %>"
    }<%
%><% if (components.includes("customThemes")) { %><%
    %>,
    CustomScssFile: "<%= componentPaths["customThemes"] %>"<%
%><% } %><%
%><% if (components.includes("variableOverrides")) { %><%
    %>,
    OverrideScssFile: "<%= componentPaths["variableOverrides"] %>"<%
%><% } %>
});

export = theme;