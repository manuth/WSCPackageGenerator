{
    "npm.exclude": "**/templates/**",
    "files.associations": {
        "**/templates/*": "ejs"
    },
    "markdown.extension.toc.levels": "1..2",
    "cSpell.enabled": false,
    "cSpell.language": "en",
    "cSpell.words": [
        "Lab",
        "Wolt",
        "WoltLab",
        "localizable"
    ],
    "mocha.requires": [
        "ts-node/register"
    ],
    "mocha.files.glob": "**/app.test.ts",
    "mocha.options": {
        "compilers": [
            "ts-node/register"
        ]
    }
}