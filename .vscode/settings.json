{
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/**.js": {
            "when": "$(basename).tsx"
        },
        "**/*.js.map": {
            "when": "$(basename)"
        }
    },
    "search.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/**.js": {
            "when": "$(basename).tsx"
        },
        "**/*.js.map": {
            "when": "$(basename)"
        }
    },
    "npm.exclude": "**/templates/**",
    "files.associations": {
        "**/templates/*": "ejs"
    },
    "markdown.extension.toc.levels": "1..2"
}