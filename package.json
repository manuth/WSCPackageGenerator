    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.9",
    "@types/npm": "^2.0.30",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "0.0.29",
    "mocha": "^6.2.0",
