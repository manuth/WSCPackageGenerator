{
  "name": "generator-wsc-package",
  "version": "2.0.2",
  "description": "A Generator for WoltLab Suite Core Packages.",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "Woltlab Suite Core",
    "Package",
    "yeoman-generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/WSCPackageGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/manuth/WSCPackageGenerator/issues"
  },
  "homepage": "https://github.com/manuth/WSCPackageGenerator#readme",
  "scripts": {
    "build": "tsc -b .",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc -b . --watch",
    "clean": "rimraf ./lib",
    "lint": "tslint -p . -t verbose && tslint -p ./WSCPackage -t verbose",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "escape-string-regexp": "^1.0.5",
    "extended-yo-generator": "^1.0.5",
    "fs-extra": "^7.0.1",
    "lodash.kebabcase": "^4.1.1",
    "upath": "^1.1.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@types/escape-string-regexp": "^1.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/lodash.kebabcase": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/npm": "^2.0.29",
    "@types/yeoman-test": "^1.7.4",
    "@types/yosay": "0.0.29",
    "mocha": "^5.2.0",
    "npm": "^6.4.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.1.2",
    "woltlab-compiler": "^1.0.1",
    "yeoman-test": "^1.9.1",
    "yo": "^2.0.5"
  }
}
