{
  "name": "@manuth/generator-wsc-package",
  "version": "3.0.1",
  "description": "A Generator for WoltLab Suite Core Packages.",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "Woltlab Suite Core",
    "Package",
    "yeoman-generator"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/manuth/WSCPackageGenerator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/WSCPackageGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/manuth/WSCPackageGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "npm run build -- --clean && rimraf ./lib",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-ide": "npm run lint || exit 0",
    "test": "mocha",
    "prepare": "npm run patch-ts && npm run rebuild",
    "patch-ts": "ts-patch install"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^11.0.6",
    "@manuth/generator-ts-project": "^3.0.13",
    "@manuth/package-json-editor": "^2.2.5",
    "@manuth/temp-files": "^2.0.8",
    "chalk": "^4.1.2",
    "dedent": "^0.7.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "lodash.kebabcase": "^4.1.1",
    "pascalcase": "^1.0.0",
    "ts-morph": "^13.0.2",
    "upath": "^2.0.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^3.2.6",
    "@manuth/extended-yo-generator-test": "^11.0.6",
    "@manuth/generator-ts-project-test": "^3.0.13",
    "@manuth/tsconfig": "^3.0.0",
    "@manuth/woltlab-compiler": "^2.1.6",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/mocha": "^9.0.0",
    "@types/mock-require": "^2.0.0",
    "@types/mute-stream": "^0.0.1",
    "@types/node": "^17.0.2",
    "@types/npm": "^7.19.0",
    "@types/pascalcase": "^1.0.1",
    "@types/sinon": "^10.0.6",
    "@types/ts-nameof": "^4.2.1",
    "@types/vscode": "^1.63.1",
    "@types/yeoman-test": "^4.0.3",
    "@types/yosay": "2.0.1",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3",
    "mock-require": "^3.0.3",
    "mock-stdin": "^1.0.0",
    "mute-stream": "^0.0.8",
    "random-js": "^2.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "source-map-support": "^0.5.21",
    "ts-nameof": "^5.0.0",
    "ts-node": "^10.4.0",
    "ts-patch": "^2.0.1",
    "types-tsconfig": "^1.2.1",
    "typescript": "^4.5.4",
    "yeoman-test": "^6.2.0",
    "yo": "^4.3.0"
  }
}
