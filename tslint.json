{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "eofline": false,
        "forin": false,
        "one-line": false,
        "trailing-comma": false,
        "no-default-export": true,
        "no-empty-interface": false,
        "no-empty": false,
        "prefer-const": false,
        "object-literal-sort-keys": false,
        "ordered-imports": true,
        "max-line-length": false,
        "arrow-parens": false,
        "array-type": false,
        "member-ordering": false,
        "no-console": false,
        "typedef": true
    }
}